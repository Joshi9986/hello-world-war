pipeline {
    agent {label 'java' }
    stages {
        stage('clone step') {
             steps {
                sh 'rm -rf hello-world-war'
                sh 'git clone https
            }
        }
        stage('Example Test') {
            agent { docker 'openjdk:8-jre' } 
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}
